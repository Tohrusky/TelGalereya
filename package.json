    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-typescript": "^3.0.0",
    "miniflare": "^3.20230904.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "wrangler": "^3.7.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.10.0",
    "canvas": "^2.11.2",
