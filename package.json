{
  "name": "tgimg-worker",
  "version": "0.0.1",
  "description": "A Cloudflare Worker for Telegraph image hosting",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Tohrusky",
  "license": "BSD-3-Clause license",
  "repository": "https://github.com/Tohrusky/tgimg-worker",
  "homepage": "https://github.com/Tohrusky/tgimg-worker",
  "scripts": {
